#!/bin/bash
~/src/qchem/gen4vasp/gen npar 20 sname staticrun generic mempernode 96 kpar 1 corespernode 20 totalnumbercores 20 ediff 1E-5 lreal .FALSE. isym -1 prec Normal kpoints "11 11 11" fm isnotmetal encut 400 istart 0 icharg 2 lorbit 11 ismear 0 sigma 0.05 prencl
~/src/qchem/gen4vasp/gen npar 20 sname nclrun1 generic ncl mempernode 96 kpar 1 corespernode 20 totalnumbercores 20 ediff 1E-5 lreal .FALSE. isym -1 prec Normal kpoints "11 11 11" fm isnotmetal encut 400 lorbit 11 ismear 0 sigma 0.05 saxis '0 0 1' #we'll figure out the best direction later
~/src/qchem/gen4vasp/gen npar 20 sname nclrun2 generic ncl mempernode 96 kpar 1 corespernode 20 totalnumbercores 20 ediff 1E-5 lreal .FALSE. isym -1 prec Normal kpoints "11 11 11" fm isnotmetal encut 400 lorbit 11 ismear 0 sigma 0.05 saxis '0 1 1' #we'll figure out the best direction later
~/src/qchem/gen4vasp/gen npar 20 sname nclrun3 generic ncl mempernode 96 kpar 1 corespernode 20 totalnumbercores 20 ediff 1E-5 lreal .FALSE. isym -1 prec Normal kpoints "11 11 11" fm isnotmetal encut 400 lorbit 11 ismear 0 sigma 0.05 saxis '1 1 1' #we'll figure out the best direction later
